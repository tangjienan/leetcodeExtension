{
  "manifest_version": 2,
  
  "name": "Leetcode random pick",
  
  "version": "0.1",

"browser_action" :{
      "default_icon" : "icon.png"
},
 "content_scripts": [{
     "js": ["jquery-3.2.1.min.js"],
     "matches": ["http://*/*", "https://*/*"]
   }]
 ,

"browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
},
 "background": {
    "scripts": ["jquery-3.2.1.min.js", "background.js"]
  },

"permissions": [
  "bookmarks",
  "tabs",
  "http://*/*", 
  "https://*/*"    
]

}